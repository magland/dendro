        {
          "name": "bin_size_msec",
          "type": "float",
          "description": "Bin size in milliseconds",
          "defaultValue": 20
        },
        {
          "name": "output_dimensions",
          "type": "int",
          "description": "Output dimensions",
          "defaultValue": 10
        }
      ],
      "attributes": []
    }
